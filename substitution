#include <stdio.h>
#include <math.h>
#include <cs50.h>
#include <ctype.h>
#include <string.h>
#include <stdlib.h>

int main(int argc, string argv[])
{

    if (argc == 2 && strlen(argv[1]) == 26 && isalpha(argv[1]) != 0)
    {
        int x = strlen(argv[1]);
        string alphabet = argv[1];

        for (int i = 0; i < 26; i++)
        {
            int letter = alphabet[i];

            printf("%i", letter);
        }
    }

    else
    {
        printf("Usage: ./substitution key");
        exit(1);
    }

    exit(1);
}
